<h1>Listing humanoids</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @humanoids.each do |humanoid| %>
      <tr>
        <td><%= link_to 'Show', humanoid %></td>
        <td><%= link_to 'Edit', edit_humanoid_path(humanoid) %></td>
        <td><%= link_to 'Destroy', humanoid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Humanoid', new_humanoid_path %>
